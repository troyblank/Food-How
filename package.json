{
  "name": "foodHow",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "build": "next build && next export",
    "start": "npm run build && cd out && serve -p 8888",
    "test": "npm run unit && npm run lint",
    "unit": "nyc --reporter=text-summary --reporter=lcov --reporter=cobertura mocha --require isomorphic-fetch --exit \"./{pages,src,test}/**/*.spec.js\"",
    "lint": "npm run eslint && npm run sasslint",
    "eslint": "eslint './{pages,recipes,src,test}/**/*.js'",
    "eslintFix": "eslint --fix './{pages,recipes,src,test}/**/*.js'",
    "sasslint": "sass-lint -c ./node_modules/@troyblank/sasslint-config-troyblank/.sass-lint.yml ./src/**/*.scss -v -q"
  },
  "nyc": {
    "temp-directory": "./coverage/.nyc_output"
  },
  "dependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/plugin-transform-modules-commonjs": "7.5.0",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.5.5",
    "@troyblank/food-how-components": "0.4.0",
    "@zeit/next-sass": "1.0.1",
    "css-loader": "1.0.1",
    "dompurify": "1.0.11",
    "lodash": "4.17.15",
    "next": "9.0.2",
    "node-sass": "4.12.0",
    "normalize.css": "8.0.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "7.1.0",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "sass-loader": "7.2.0",
    "webpack": "4.39.1",
    "jquery": "2.2.4",
    "nunjucks": "1.0.1"
  },
  "devDependencies": {
    "@troyblank/eslint-config-troyblank": "1.1.0",
    "@troyblank/mocha-base-troyblank": "1.0.0",
    "@troyblank/sasslint-config-troyblank": "1.0.0",
    "coveralls": "3.0.6",
    "eslint": "6.1.0",
    "isomorphic-fetch": "2.2.1",
    "sass-lint": "1.13.1",
    "serve": "11.1.0"
  },
  "author": "Troy Blank",
  "license": "BSD"
}
