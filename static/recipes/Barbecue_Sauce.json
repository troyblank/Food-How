    "ingredients": [
        "Juice of 1 lime",
        "30 ozs tomato puree",
        "2 TBS soy sauce",
        "2 TBS black molasses",
        "2 TBS butter",
        "2 TBS chili powder",
        "4 TBS Cumin",
        "2 tsp black pepper",
        "1 tsp salt",
        "&frac12; tsp cayenne pepper"
    ],
    "directions": [
        "Put all ingredients in a pot.",
        "Cook on med-high for 10 mins."
    ]
