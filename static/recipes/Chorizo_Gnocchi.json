{
    "title": "Chorizo Gnocchi",
    "ingredients": [
        "[1 &frac12; cup Gnocchi](/recipe/gnocchi)",
        "&frac12; lb chorizo",
        "1 red bell pepper (diced)",
        "2 cups spinach",
        "&frac12; yellow onion (sliced)",
        "3 garlic cloves (minced)",
        "1 TBSP olive oil",
        "1 tsp italian seasoning",
        "1 tsp salt"
    ],
    "directions": [
        { "text": "sauté chorizo with oil, onion, pepper, and gnocchi for 4 mins on high heat." },
        { "text": "Mix in spinach, garlic, and spices and sauté for 2 additional mins." }
    ]
}